{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/api/clients", "function": "clients" },
      { "source": "/api/businesses", "function": "businesses" },
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
